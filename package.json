{
  "name": "utah-air-quality-alexa-skill",
  "version": "0.1.0",
  "description": "An Alexa skill for determining what the air quality it for your county in Utah.",
  "keywords": [
    "burn",
    "day",
    "utah",
    "deq",
    "air",
    "quality"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stdavis/utah-air-quality-alexa-skill.git"
  },
  "license": "MIT",
  "author": "Scott Davis <sthomasdavis@gmail.com>",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "deploy": "dpl",
    "format": "prettier . --write",
    "lint": "eslint .",
    "test": "npm run lint && mocha"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "eslintConfig": {
    "env": {
      "mocha": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ]
  },
  "dependencies": {
    "ask-sdk": "^2.12.1",
    "csv-parse": "^5.3.0",
    "dotenv": "^16.0.1",
    "got": "^12.3.1",
    "lodash.capitalize": "4.2.1",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "dpl": "^4.0.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.0",
    "prettier-plugin-packagejson": "^2.2.18"
  },
  "files_to_deploy": [
    "package.json",
    "airquality.js",
    "index.js",
    "parser.js",
    "getcounty.js"
  ]
}
